{
  "name": "dart-tournament-manager",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "dev:with-scraper": "concurrently \"vite\" \"node ../backend/scraper-control.js\"",
    "build": "vite build",
    "build:electron": "cross-env ELECTRON=true vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm run dev\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "npm run build:electron && electron-builder",
    "electron:build:win": "npm run build:electron && electron-builder --win",
    "pack": "npm run build:electron && electron-builder --dir",
    "dist": "npm run build:electron && electron-builder",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "@tauri-apps/api": "^2.9.1",
    "axios": "^1.6.0",
    "brackets-manager": "^1.8.2",
    "brackets-model": "^1.6.0",
    "brackets-viewer": "^1.9.0",
    "lucide-react": "^0.295.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.9.6",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "gh-pages": "^6.1.1",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "vite": "^5.0.0",
    "wait-on": "^9.0.3"
  }
}
